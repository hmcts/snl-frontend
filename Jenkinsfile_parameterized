#!groovy
@Library("Infrastructure") _

def type = "angular"
def product = "snl"
def component = "notes"

withPipeline(type, product, component) {
    afterCheckout {
        sh "yarn cache clean"
    }

    echo env.CHANGE_TITLE
    if (env.CHANGE_TITLE && !env.CHANGE_TITLE.contains('[PREVIEW]')
            && env.CHANGE_TITLE.contains('[AKS]')) {
        enableDockerBuild()
        enableDeployToAKS()
    }
}
