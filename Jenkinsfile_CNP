#!groovy

properties(
    [[$class: 'GithubProjectProperty', projectUrlStr: 'https://github.com/hmcts/snl-frontend/'],
     pipelineTriggers([[$class: 'GitHubPushTrigger']])]
)

@Library("Infrastructure")

def product = "snl"
def app = "frontend"


withPipeline("angular", product, app) {

     before('buildinfra:aat') {
       error 'This is PoC project for Scheduling and Listing therefore the environments will not be built'
     }

}