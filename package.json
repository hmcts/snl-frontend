{
  "name": "snl-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "postinstall": "webdriver-manager update",
    "build": "ng build --prod",
    "start": "ng serve --proxy-config proxy.config.json",
    "test": "ng test --single-run --code-coverage",
    "test:coverage": "istanbul check-coverage --statements 0 --lines 0 --functions 0 --branches 0",
    "test:a11y": "echo 'TODO: Accessibility tests'",
    "test:nsp": "nsp check",
    "test:smoke": "protractor e2e/protractor.conf.js --suite smoke",
    "test:smokeDocker": "echo 'TODO: Smoke docker",
    "test:functional": "echo 'TODO: Functional tests'",
    "lint": "tslint --project . -t verbose 'src/**/*.ts'",
    "e2e": "protractor e2e/protractor.conf.js --suite e2e",
    "start:fr": "ng serve --aot --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build:fr": "ng build --prod --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "extract": "ng xi18n --outputPath=src/locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "sonar-scan": "NODE_PATH=. sonar-scanner -X"
  },
  "dependencies": {
    "@angular/animations": "~5.0.0",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/compiler-cli": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "~5.0.0",
    "@angular/http": "~5.0.0",
    "@angular/material": "^5.2.5",
    "@angular/material-moment-adapter": "^6.3.3",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@angular/platform-server": "~5.0.0",
    "@angular/router": "~5.0.0",
    "@angular/upgrade": "~5.0.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/entity": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@types/jquery": "3.2.7",
    "class-transformer": "^0.1.8",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "font-awesome": "^4.7.0",
    "fullcalendar": "~3.9.0",
    "fullcalendar-scheduler": "^1.9.4",
    "govuk-elements-sass": "^3.1.2",
    "govuk_template_mustache": "^0.23.0",
    "http-status-codes": "^1.3.0",
    "jasmine-marbles": "^0.2.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "mocha": "^5.0.5",
    "moment": "^2.22.1",
    "ngx-md": "^3.1.1",
    "ngx-pagination": "^3.0.3",
    "normalizr": "^3.2.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.0",
    "ts-loader": "^3.1.1",
    "url": "^0.11.0",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@ngrx/schematics": "^5.2.0",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.45",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.12.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "0.2.1",
    "lodash": "^4.16.2",
    "ng2-mock-component": "^0.1.1",
    "nsp": "^3.2.1",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "^5.1.1",
    "protractor-beautiful-reporter": "^1.2.1",
    "puppeteer": "^1.5.0",
    "sonar-scanner": "^3.0.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.0.0",
    "typescript": "2.9.2",
    "webdriver-manager": "^12.0.6"
  }
}
