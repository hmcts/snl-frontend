{
  "name": "snl-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "postinstall": "webdriver-manager update",
    "build": "ng build --prod",
    "start": "ng serve --proxy-config proxy.config.json",
    "test": "ng test --code-coverage --watch=false",
    "test:coverage": "istanbul check-coverage --statements 0 --lines 0 --functions 0 --branches 0",
    "test:a11y": "echo 'TODO: Accessibility tests'",
    "test:nsp": "nsp check",
    "test:smoke": "protractor e2e/protractor.conf.js --suite smoke",
    "test:smokeDocker": "echo 'TODO: Smoke docker",
    "test:functional": "npm run e2e",
    "lint": "tslint --project . -t verbose 'src/**/*.ts'",
    "e2e": "protractor e2e/protractor.conf.js --suite e2e",
    "start:fr": "ng serve --aot --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build:fr": "ng build --prod --i18sudo nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "extract": "ng xi18n --outputPath=src/locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "sonar-scan": "NODE_PATH=. sonar-scanner -X"
  },
  "dependencies": {
    "@angular/animations": "~6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.0",
    "@angular/compiler-cli": "~6.1.0",
    "@angular/core": "~6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/material": "^6.4.1",
    "@angular/material-moment-adapter": "^6.4.1",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/platform-server": "~6.1.0",
    "@angular/router": "~6.1.0",
    "@angular/upgrade": "~6.1.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/jquery": "3.3.5",
    "class-transformer": "^0.1.8",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "fullcalendar": "~3.9.0",
    "fullcalendar-scheduler": "^1.9.4",
    "jasmine-marbles": "^0.3.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "ngx-md": "^6.0.0",
    "ngx-pagination": "^3.1.1",
    "normalizr": "^3.2.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "ts-loader": "^4.4.2",
    "url": "^0.11.0",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.5.3",
    "codelyzer": "^4.4.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.14.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "lodash": "^4.17.10",
    "ng2-mock-component": "^0.1.1",
    "nsp": "^3.2.1",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "^5.4.0",
    "protractor-beautiful-reporter": "^1.2.3",
    "puppeteer": "1.6.1",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "2.9.2",
    "webdriver-manager": "^12.1.0"
  }
}
